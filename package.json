{
  "name": "web-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install": "yarn && cd smart-contracts && yarn",
    "dev": "concurrently \"next dev\" \"cd smart-contracts && yarn watch compilation\"",
    "build": "echo $PUBSUB_KEYFILE > pubsub-keyfile.json && yarn compile && next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "cd smart-contracts && yarn compile"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.17.0",
    "ethers": "^5.4.6",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.20",
    "concurrently": "^6.2.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "typescript": "4.4.2"
  }
}
